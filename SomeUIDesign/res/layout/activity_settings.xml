<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
 <RelativeLayout
     android:layout_width="fill_parent"
     android:layout_height="476dp"
     android:background="@drawable/settings1" >
      
         <ImageButton
             android:id="@+id/back12"
             android:layout_width="60dp"
             android:layout_height="27dp"
             android:layout_alignParentLeft="true"
            
             android:layout_alignParentTop="true"
             android:layout_marginTop="0dp"
               android:layout_marginLeft="1dp"
             android:onClick="backclick12"
             android:padding="0dp"
             android:src="@drawable/newback" />

         <LinearLayout
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:layout_alignParentLeft="true"
             android:layout_below="@+id/back12"
            
             android:layout_marginTop="0dp"
             android:orientation="vertical" >

             <Button
                 android:id="@+id/btnset1"
                 android:layout_width="match_parent"
                 android:layout_height="45dp"
                 android:alpha="0"
                 android:onClick="setclick1"
                 />
             <Button
                 android:id="@+id/btnset2"
                 android:layout_width="match_parent"
                 android:layout_height="40dp"
                 android:alpha="0"
                 android:onClick="setclick2"
                 />
             <Button
                 android:id="@+id/btnset3"
                 android:layout_width="match_parent"
                 android:layout_height="40dp"
                 android:alpha="0"
                 android:onClick="setclick3"
                 />
             <Button
                 android:id="@+id/btnset4"
                 android:layout_width="match_parent"
                 android:layout_height="40dp"
                 android:alpha="0"
                 android:onClick="setclick4"
                 />
             <Button
                 android:id="@+id/btnset5"
                 android:layout_width="match_parent"
                 android:layout_height="40dp"
                 android:alpha="0"
                 android:onClick="setclick5"
                 />
             <Button
                 android:id="@+id/btnset6"
                 android:layout_width="match_parent"
                 android:layout_height="55dp"
                 android:alpha="0"
                 android:onClick="setclick6"
                 />
             <Button
                 android:id="@+id/btnset7"
                 android:layout_width="match_parent"
                 android:layout_height="40dp"
                 android:alpha="0"
                 android:onClick="setclick7"
                 />
         </LinearLayout>

        </RelativeLayout>
</LinearLayout>
