<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
     <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@drawable/recentlyview" >
        
      
         <ImageButton
             android:id="@+id/back10"
             android:layout_width="60dp"
             android:layout_height="27dp"
             android:layout_alignParentLeft="true"
            
             android:layout_alignParentTop="true"
             android:layout_marginTop="0dp"
               android:layout_marginLeft="1dp"
             android:onClick="backclick10"
             android:padding="0dp"
             android:src="@drawable/newback" />

         <Button
             android:id="@+id/btndetail5"
             android:layout_width="match_parent"
             android:layout_height="80dp"
             android:layout_alignLeft="@+id/back10"
             android:layout_below="@+id/back10"
             android:layout_marginLeft="18dp"
             android:layout_marginTop="16dp"
             android:alpha="0"
             android:onClick="detailclick5"
              />

        </RelativeLayout>

</LinearLayout>
